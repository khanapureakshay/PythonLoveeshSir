   "metadata": {},
   "outputs": [],
   "source": [
    "# To find Cube:\n",
    "# Using Define Function\n",
    "def cube(i):\n",
    "    return i*i*i\n",
    "\n",
    "# Using Lambda Function\n",
    "lambda_cube= lambda i : i*i*i\n",
    "\n",
    "print(cube(25))\n",
    "print(lambda_cube(25))"
   ]
